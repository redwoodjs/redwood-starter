    "@redwoodjs/structure": "1.0.0-rc.3",
    "boxen": "5.1.2",
    "camelcase": "6.2.1",
    "chalk": "4.1.2",
    "concurrently": "6.4.0",
    "configstore": "3.1.5",
    "core-js": "3.19.1",
    "cross-env": "7.0.3",
    "decamelize": "5.0.0",
    "dotenv-defaults": "3.0.0",
    "envinfo": "7.8.1",
    "execa": "5.1.1",
